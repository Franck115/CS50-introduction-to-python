# Ask the question

def ask_question():
    question = input("What is the answer to the Great Question of Life, the Universe and Everything? ")
    return question


def main():
    question = ask_question()
    if question.strip() == "42":
     print("Yes")
    elif question.lower().strip() == "forty-two":
     print("Yes")
    elif question.lower().strip() == "forty two":
     print("Yes")
    else:
     print("No")


main()
